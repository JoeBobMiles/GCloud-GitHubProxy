{
  "name": "gcloud-gh-proxy",
  "private": true,
  "description": "A GCloud Function that serves as an API Proxy for GitHub.",
  "author": {
    "name": "Joseph R Miles",
    "email": "me@josephrmiles.com",
    "url": "https://josephrmiles.com"
  },
  "license": "MIT",
  "version": "0.1.0",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joebobmiles/GCloud-GitHubProxy.git"
  },
  "scripts": {
    "start": "functions-framework --target=ghProxy --source=functions/",
    "build": "tsc",
    "deploy": "npm run build && gcloud functions deploy ghProxy --runtime nodejs8 --trigger-http --allow-unauthenticated --source functions/"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.5.1",
    "@types/express": "^4.17.6",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
